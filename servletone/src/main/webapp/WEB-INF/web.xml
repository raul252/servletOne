<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <servlet>
    <servlet-name>ServletOne</servlet-name>
    <servlet-class>com.example.one.servletone.ServletOne</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>ServletTwo</servlet-name>
    <servlet-class>com.example.one.servletone.ServletTwo</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletOne</servlet-name>
    <url-pattern>/servlet/servletOne</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ServletTwo</servlet-name>
    <url-pattern>/servlet/servletTwo</url-pattern>
  </servlet-mapping>
</web-app>